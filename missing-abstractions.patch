+            return Err(EFAULT);
+        }
+        let res = unsafe { bindings::copy_to_user(self.ptr as *mut core::ffi::c_void, data as _, len as _) };
+        if res != 0 {
+            return Err(EFAULT);
+        }
+        // Since this is not a pointer to a valid object in our program,
+        // we cannot use `add`, which has C-style rules for defined
+        // behavior.
+        self.ptr = self.ptr.wrapping_add(len);
+        self.length -= len;
+        Ok(())
